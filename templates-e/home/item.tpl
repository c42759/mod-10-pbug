<tr>
	<td>#{c2r-id}</td>
	<td>
		<a href="{c2r-path-bo}/{c2r-lg}/{c2r-module-folder}/project/{c2r-id}">{c2r-title}</a>
		<br>
		<small>Created {c2r-date}</small>
	</td>
	<td>
		<ul class="list-inline">
			<li>
				<img src="https://colorlib.com/polygon/gentelella/images/user.png" class="img-thumbnail xs-block15 sm-block60" alt="Avatar">
			</li>
			<li>
				<img src="https://colorlib.com/polygon/gentelella/images/user.png" class="img-thumbnail xs-block15 sm-block60" alt="Avatar">
			</li>
			<li>
				<img src="https://colorlib.com/polygon/gentelella/images/user.png" class="img-thumbnail xs-block15 sm-block60" alt="Avatar">
			</li>
			<li>
				<img src="https://colorlib.com/polygon/gentelella/images/user.png" class="img-thumbnail xs-block15 sm-block60" alt="Avatar">
			</li>
		</ul>
	</td>
	<td class="project_progress">
		<div class="progress progress_sm">
			<div class="progress-bar bg-green" role="progressbar" data-transitiongoal="57" aria-valuenow="55" style="width: 57%;"></div>
		</div>
		<small>57% Complete</small>
	</td>
	<td>
		<button type="button" class="btn btn-success btn-xs">Success</button>
	</td>
	<td>
		<a href="{c2r-path-bo}/{c2r-lg}/{c2r-module-folder}/project/{c2r-id}" class="btn btn-primary btn-sm"><i class="fa fa-folder"></i> View </a>&nbsp;
		<a href="{c2r-path-bo}/{c2r-lg}/{c2r-module-folder}/project-edit/{c2r-id}" class="btn btn-edit btn-sm"><i class="fa fa-pencil"></i> Edit </a>&nbsp;
		<a href="{c2r-path-bo}/{c2r-lg}/{c2r-module-folder}/project-delete/{c2r-id}" class="btn btn-del btn-sm"><i class="fa fa-trash-o"></i></a>
	</td>
</tr>
